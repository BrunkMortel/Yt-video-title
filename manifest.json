{
    "name": "Youtube music title video",
    "description": "Display the current music title on OBS",
    "version": "1.0",
    "manifest_version": 3,

    "permissions": ["activeTab", "scripting","storage","tabs"],

    "background": {
        "service_worker": "background.js"
      },

      "action": {
        "default_popup": "popup.html"
      }
}